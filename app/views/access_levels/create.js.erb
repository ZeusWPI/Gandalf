<% if @access_level.errors.any? %>
  $('#access-level-errors').html("<%= escape_javascript form_errors(@access_level) %>")
<% else %>
  $('#access-level-errors').html('')
  $('#new_access_level')[0].reset()

  <%# break advanced deliberately, how we currently do it will not continue working! %>
  $('#table-access-levels tr:last').before('<%= escape_javascript render @access_level %>')
<% end %>
