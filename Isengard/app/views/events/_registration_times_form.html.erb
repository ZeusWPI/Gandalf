<%= form_for @event, method: :patch do |f| %>
   <%= form_date_field f, :registration_open_date, "registration-start", datepicker_time(f.object.registration_open_date) %>
   <%= form_date_field f, :registration_close_date, "registration-end", datepicker_time(f.object.registration_close_date) %>
   <%= form_check_box f, :show_ticket_count %>
   <%= f.button :submit, class: "btn btn-primary" %>
<% end %>
