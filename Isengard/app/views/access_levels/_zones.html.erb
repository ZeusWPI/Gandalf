<%= form_for [access_level.event, access_level],
  url: event_access_level_update_zones_path(access_level.event, access_level),
  method: :patch,
  remote: true do |f| %>
  <%= f.collection_check_boxes :zones, access_level.event.zones, lambda {|z| [z.id, access_level.id]}, :name,  checked: access_level.zones.map {|z| [z.id, access_level.id]} %>
<% end %>
